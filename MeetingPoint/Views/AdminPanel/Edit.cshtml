@model EditUserModel
@{
    ViewData["Title"] = "Редактирование пользователя";
}
<form asp-action="Edit" asp-controller="AdminPanel">
    <div asp-validation-summary="All"></div>
    <div>
        <input type="hidden" asp-for="Id" />
    </div>
    <div>
        <label asp-for="Nickname">Nickname</label>
        <input type="text" asp-for="Nickname"/>
    </div>
    <div>
        <input type="submit" value="Сохранить" />
    </div>
</form>
